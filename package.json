{
  "name": "@fireguard/jsdoc-template",
  "version": "1.0.0",
  "description": "A clean, responsive documentation template theme for JSDoc 3 forked by Docdash",
  "main": "publish.js",
  "scripts": {
    "test": "jsdoc -c fixtures/fixtures.conf.json",
    "sync": "browser-sync start -s ../fixtures-doc -f ../fixtures-doc --reload-delay 1000 --no-ui --no-notify",
    "watch": "watch-run -d 1000 -p tmpl/**,static/** \"npm run test\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fireguard/jsdoc-template.git"
  },
  "devDependencies": {
    "jsdoc": "latest",
    "browser-sync": "latest",
    "watch-run": "latest"
  },
  "author": "Ronaldo Meneguite <ronaldo@fireguard.com.br>",
  "license": "Apache-2.0",
  "keywords": [
    "jsdoc",
    "template"
  ],
  "files": [
    "publish.js",
    "static",
    "tmpl"
  ]
}
